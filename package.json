    "html-webpack-plugin": "^3.2.0",
    "nodemon": "^1.18.6",
    "vue": "^2.5.17",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  },
  "dependencies": {
